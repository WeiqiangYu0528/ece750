{
  "name": "instagram",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.7.10",
    "@heroicons/react": "^2.0.14",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "axios": "^1.3.2",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "delay-cli": "^2.0.0",
    "dotenv": "^16.0.3",
    "emoji-picker-react": "^4.4.8",
    "enzyme": "^3.11.0",
    "express": "^4.18.2",
    "mongodb": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading-skeleton": "^3.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build:css": "postcss src/styles/tailwind.css -o src/styles/app.css",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/app.css --watch",
    "react-scripts:start": "delay 2 && react-scripts start",
    "react-scripts:dist": "react-scripts build",
    "start": "run-p watch:css react-scripts:start",
    "build": "run-s build:css react-scripts:dist",
    "test": "react-scripts test --coverage --watchAll",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!<rootDir>/src/App.tsx",
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/src/react-app-env.d.ts",
      "!<rootDir>/src/reportWebVitals.ts",
      "!<rootDir>/src/config/axiosConfig.js",
      "!<rootDir>/src/components/sidebar/modal.tsx",
      "!<rootDir>/src/pages/hashtag.tsx",
      "!<rootDir>/src/pages/explore.tsx",
      "!<rootDir>/src/pages/userProfile.tsx",
      "!<rootDir>/src/pages/dashboard.tsx",
      "!<rootDir>/src/pages/error-page.tsx",
      "!<rootDir>/src/pages/userProfile.tsx",
      "!<rootDir>/src/components/sidebar/sidebar.tsx",
      "!<rootDir>/src/components/sidebar/SwitchModal.tsx",
      "!<rootDir>/src/components/post/modal-emoji.tsx",
      "!<rootDir>/src/components/sidebar/thumbnail.tsx",
      "!<rootDir>/src/components/search/searchBar.tsx",
      "!<rootDir>/src/components/notification/notificationBar.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "html",
      "text"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/user-event": "^14.4.3",
    "autoprefixer": "^10.4.13",
    "intersection-observer": "^0.12.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "react-intersection-observer": "^9.4.2",
    "tailwindcss": "^3.2.4"
  }
}
